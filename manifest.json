{
  "action": {
    "default_icon": {
      "128": "assets/delete.png",
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "My Youtube Bookmarks"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "css": ["content.css"],
      "js": ["youtubeContentScript.js"],
      "matches": ["https://*.youtube.com/*"],
      "run_at": "document_end"
    },
    {
      "js": ["allContentScript.js"],
      "matches": ["*://*/*"],
      "run_at": "document_end"
    }
  ],
  "description": "It helps you save timestamps in Youtube videos",
  "host_permissions": ["https://*.youtube.com/*"],
  "key": "",
  "manifest_version": 3,
  "name": "Youtube Timestamp Saver",
  "options_page": "options.html",
  "permissions": ["storage", "tabs", "contextMenus"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.1.0",
  "web_accessible_resources": [
    {
      "matches": ["*://*/*"],
      "resources": [
        "*.html",
        "assets/bookmark.png",
        "assets/bookmark.gif",
        "assets/play.png",
        "assets/delete.png",
        "assets/save.png",
        "assets/2.png",
        "assets/3.png",
        "assets/4.png",
        "assets/5.png",
        "assets/6.png",
        "assets/yt.png",
        "assets/globe.svg",
        "assets/raven.svg"
      ]
    }
  ],
  "icons": {
    "16": "assets/save16.png",
    "48": "assets/save48.png"
  }
}
